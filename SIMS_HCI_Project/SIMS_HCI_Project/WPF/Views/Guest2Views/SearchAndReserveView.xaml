<Page x:Class="SIMS_HCI_Project.WPF.Views.Guest2Views.SearchAndReserveView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SIMS_HCI_Project.WPF.ViewModels.Guest2ViewModels"
      mc:Ignorable="d" 
      Height="518" Width="1080"
      Title="SearchAndReserveView">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="350"/>
            <RowDefinition Height="168"/>
        </Grid.RowDefinitions>
        <Button Content="Back" Command="{Binding Back}" HorizontalAlignment="Left" VerticalAlignment="Top"  BorderBrush="#FFEDDCDC" Height="33" Width="68"/>
        <Button Content="HELP"  HorizontalAlignment="Right" VerticalAlignment="Top" Height="33" Width="68"/>

        <Frame x:Name="TopFrame" Grid.Row="0" Width="1080" Margin="0,38,0,0"/>
        <Frame x:Name="BottomFrame" Grid.Row="1" Width="1080" Height="168"/>
    </Grid>
</Page>
